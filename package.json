{
  "name": "@yagisumi/n7zip",
  "version": "0.1.0",
  "description": "A Node.js wrapper of 7zip with 7z.dll(7z.so)",
  "keywords": [
    "7zip",
    "zip",
    "rar"
  ],
  "engines": {
    "node": ">= 8.16.0"
  },
  "private": true,
  "main": "lib/n7zip",
  "types": "lib/n7zip.d.ts",
  "scripts": {
    "test": "run-s prettier:fix eslint jest",
    "test:build": "node-gyp rebuild --debug",
    "jest": "jest",
    "install": "node-gyp rebuild --debug",
    "build": "run-s clean build:*",
    "clean": "rimraf lib coverage umd *.tgz .rpt2_cache",
    "build:cjs": "rollup -c -f cjs -o ./lib/n7zip.js",
    "build:d_ts": "tsc -p src -d --emitDeclarationOnly --outDir ./lib",
    "prettier:fix": "prettier --write **/*.{ts,js,json}",
    "eslint": "eslint **/*.{js,ts}",
    "prepack": "run-s build test"
  },
  "author": "yagisumi",
  "repository": "github:yagisumi/node-n7zip",
  "homepage": "http://github.com/yagisumi/node-n7zip#readme",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^25.1.3",
    "@types/node": "^13.9.0",
    "@typescript-eslint/eslint-plugin": "^2.21.0",
    "@typescript-eslint/parser": "^2.21.0",
    "coveralls": "^3.0.9",
    "eslint": "^6.8.0",
    "jest": "^25.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.2",
    "rollup": "^1.32.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-terser": "^5.2.0",
    "rollup-plugin-typescript2": "^0.26.0",
    "ts-jest": "^25.2.1",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "bindings": "^1.5.0",
    "node-addon-api": "^2.0.0"
  }
}
